{
  "name": "react-shop",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean:cache": "rm -rf ./node_modules/.vite_cache ./node_modules/.vite",
    "clean:deps": "rm -rf ./node_modules package-lock.json",
    "clean:build": "rm -rf dist",
    "clean": "npm run clean:cache && npm run clean:deps && npm run clean:build",
    "setup": "npm run clean && npm install",
    "dev": "vite --force",
    "start": "npm run dev",
    "reset": "npm run clean:all && rm -f package-lock.json && npm install",
    "clean:all": "rm -rf node_modules/.vite node_modules/.cache dist",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "install": "echo 'Dependencies installed successfully!'"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.8.3",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^7.3.0",
    "recoil": "^0.7.7",
    "react-toastify": "^11.0.5",
    "@tanstack/react-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.68.0",
    "react-error-boundary": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "postcss": "^8.4.31",
    "postcss-nesting": "^13.0.1",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "terser": "^5.39.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "resolutions": {
    "glob": "^9.0.0",
    "rimraf": "^5.0.0"
  },
  "overrides": {
    "glob": "^9.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
